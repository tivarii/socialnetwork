# Example environment variables - Copy to .env and update with your values

NODE_ENV=development
PORT=5000

# JWT Configuration
JWT_SECRET=your_super_secret_jwt_key_here_change_in_production_minimum_32_characters
JWT_EXPIRES_IN=7d

